# What information was recalled or generated by your collegue bart?

PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX prov: <http://www.w3.org/ns/prov#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX tido: <https://w3id.org/tido#>
PREFIX tido_case: <https://w3id.org/tido/case#>
PREFIX di01: <https://w3id.org/tido/di01#>
PREFIX di02: <https://w3id.org/tido/di02#>

Select (group_concat(distinct ?info_class;separator=" | ") as ?inf_classes) ?i_value
Where {
    bind(tido_case:bart as ?colleague)
    bind(tido_case:case_1 as ?case)
    ?activity tido:contributesTo ?case ;
    	prov:wasAssociatedWith ?colleague .
    ?poi prov:wasGeneratedBy|tido:wasRecalledBy ?activity ;
    		a ?info_class ;
    		rdf:value ?i_value .
   	?info_class rdfs:subClassOf* tido:PieceOfInformation .
} Group by ?i_value