@prefix : <https://w3id.org/tido#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix mod: <https://w3id.org/mod#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix urref: <http://eturwg.c4i.gmu.edu/files/ontologies/URREF_v5_dev.owl#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

dcterms:created a owl:AnnotationProperty .

dcterms:description a owl:AnnotationProperty .

dcterms:license a owl:AnnotationProperty .

dcterms:title a owl:AnnotationProperty .

vann:preferredNamespacePrefix a owl:AnnotationProperty .

vann:preferredNamespaceUri a owl:AnnotationProperty .

owl:versionIRI a owl:AnnotationProperty .

owl:versionInfo a owl:AnnotationProperty .

<https://w3id.org/tido> a owl:Ontology ;
    dcterms:created "2025-05-07"^^xsd:date ;
    dcterms:description "The TIDO ontology can be used to describe the decision processes within the threat intelligence domain"@en ;
    dcterms:license <https://creativecommons.org/licenses/by/4.0/> ;
    dcterms:title "The Threat Intelligence Decision Ontology"@en ;
    vann:preferredNamespacePrefix "tido" ;
    vann:preferredNamespaceUri : ;
    owl:versionIRI <https://w3id.org/tido/1.0.0> ;
    owl:versionInfo "1.0.0" ;
    mod:createdWith <https://chowlk.linkeddata.es/> .

:Agent a owl:Class ;
    rdfs:label "Agent" ;
    rdfs:subClassOf prov:Agent .

:Cost a owl:Class ;
    rdfs:label "Cost" ;
    rdfs:subClassOf :Consideration .

:Goal a owl:Class ;
    rdfs:label "Goal" ;
    rdfs:subClassOf :Consideration .

:answers a owl:ObjectProperty ;
    rdfs:label "answers" ;
    rdfs:domain :PieceOfInformation ;
    rdfs:range :RQ .

:assumes a owl:ObjectProperty ;
    rdfs:label "assumes" ;
    rdfs:domain :Option ;
    rdfs:range :Hypothesis .

:disputes a owl:ObjectProperty ;
    rdfs:label "disputes" ;
    rdfs:domain :PieceOfInformation ;
    rdfs:range :Hypothesis ;
    rdfs:subPropertyOf :providesInsightsInto .

:hasConsideration a owl:ObjectProperty ;
    rdfs:label "has consideration" ;
    rdfs:domain :Option ;
    rdfs:range :Consideration .

:hasContext a owl:ObjectProperty ;
    rdfs:label "has context" ;
    rdfs:domain :PieceOfInformation ;
    rdfs:range :PieceOfInformation .

:informs a owl:ObjectProperty ;
    rdfs:label "informs" ;
    rdfs:domain :Option ;
    rdfs:range :RQ .

:questions a owl:ObjectProperty ;
    rdfs:label "questions" ;
    rdfs:domain :RQ ;
    rdfs:range :PieceOfInformation .

:supports a owl:ObjectProperty ;
    rdfs:label "supports" ;
    rdfs:domain :PieceOfInformation ;
    rdfs:range :Hypothesis ;
    rdfs:subPropertyOf :providesInsightsInto .

:wasExecutedBy a owl:ObjectProperty ;
    rdfs:label "was executed by" ;
    rdfs:domain :Option ;
    rdfs:range :Activity .

:wasRecalledBy a owl:ObjectProperty ;
    rdfs:label "was recalled by" ;
    rdfs:domain :PieceOfInformation ;
    rdfs:range :Orientation .

:wasSelectedBy a owl:ObjectProperty ;
    rdfs:label "was selected by" ;
    rdfs:domain :Option ;
    rdfs:range :Resolution .

urref:Information a owl:Class ;
    rdfs:label "Information" .

prov:Activity a owl:Class ;
    rdfs:label "Activity" .

prov:Agent a owl:Class ;
    rdfs:label "Agent" .

prov:Collection a owl:Class ;
    rdfs:label "Collection" .

prov:Entity a owl:Class ;
    rdfs:label "Entity" .

prov:wasMemberOf a owl:ObjectProperty ;
    rdfs:label "was member of" .

:Evidence a owl:Class ;
    rdfs:label "Evidence" ;
    rdfs:subClassOf :PieceOfInformation ;
    owl:disjointWith :Hypothesis .

:Orientation a owl:Class ;
    rdfs:label "Orientation" ;
    rdfs:subClassOf :Activity .

:Resolution a owl:Class ;
    rdfs:label "Resolution" ;
    rdfs:subClassOf :Activity .

:contributesTo a owl:ObjectProperty ;
    rdfs:label "contributes to" ;
    rdfs:domain :Activity ;
    rdfs:range :Case .

:investigates a owl:ObjectProperty ;
    rdfs:label "investigates" ;
    rdfs:domain :Case ;
    rdfs:range :RQ .

:providesInsightsInto a owl:ObjectProperty ;
    rdfs:label "provides insights into" ;
    rdfs:domain :PieceOfInformation ;
    rdfs:range :Hypothesis .

:Consideration a owl:Class ;
    rdfs:label "Consideration" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onClass :Evaluation ;
            owl:onProperty prov:wasGeneratedBy ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        :PieceOfInformation .

prov:wasGeneratedBy a owl:ObjectProperty ;
    rdfs:label "was generated by" .

:Case a owl:Class ;
    rdfs:label "Case" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass :RQ ;
            owl:onProperty :investigates ],
        prov:Collection .

:Evaluation a owl:Class ;
    rdfs:label "Evaluation" ;
    rdfs:subClassOf :Activity .

:Activity a owl:Class ;
    rdfs:label "Activity" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass :Case ;
            owl:onProperty :contributesTo ],
        prov:Activity .

:Hypothesis a owl:Class ;
    rdfs:label "Hypothesis" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onClass :Evaluation ;
            owl:onProperty prov:wasGeneratedBy ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        :PieceOfInformation ;
    owl:disjointWith :Evidence .

:Option a owl:Class ;
    rdfs:label "Option" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onClass :Evaluation ;
            owl:onProperty prov:wasGeneratedBy ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        :PieceOfInformation .

:RQ a owl:Class ;
    rdfs:label "R Q" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onClass :Evaluation ;
            owl:onProperty prov:wasGeneratedBy ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        :PieceOfInformation .

:PieceOfInformation a owl:Class ;
    rdfs:label "Piece Of Information" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass :Case ;
            owl:onProperty prov:wasMemberOf ],
        urref:Information,
        prov:Entity .

